{
  "extends": "airbnb",
  "env": {"node": true, "es6": true},
  "settings": {
    "import/resolver": {
      "alias": [
        ["components", "./src/components"]
      ],
      "webpack": {
        "config": {
          "node": {
            "paths": ["src"],
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          },
          "resolve": {
            "extensions": [".js", ".jsx"]
          }
        }
      }
    }
  },

}